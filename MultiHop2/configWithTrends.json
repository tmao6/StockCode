{
	"data": {
		"filename": "data/joined.csv",
		"columns": [
			"Close",
			"Open",
			"High",
			"Low",
			"Volume",
			"Advertising",
			"Alternative Carriers",
			"Broadcasting",
			"Cable",
			"Interactive Media",
			"Movies",
			"Publishing"
		],
		"sequence_length": 10,
		"train_test_split": 0.85,
		"normalise": true
	},
	"training": {
		"epochs": 7,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 9,
				"input_dim": 12,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
